<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cinzaClaro"
    tools:context="com.realvibedev.paoquente.Padaria.PainelAdministrativo">

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/amarelologo">


        <TextView
            android:id="@+id/textView24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:fontFamily="serif"
            android:gravity="center"
            android:text="Painel Operacional"
            android:textColor="@android:color/white"
            android:textSize="24sp" />

        <ImageView
            android:id="@+id/imageView30"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginEnd="5dp"
            android:layout_marginRight="5dp"
            app:srcCompat="@mipmap/menu" />

    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/relativeLayout">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingBottom="10dp">

            <RelativeLayout
                android:id="@+id/relativeLayout10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginEnd="5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="10dp"
                android:background="@android:color/white"
                android:paddingBottom="20dp">

                <TextView
                    android:id="@+id/textView27"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"
                    android:fontFamily="serif"
                    android:text="Últimas fornadas"
                    android:textColor="@android:color/black"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/textView71"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textView27"
                    android:layout_alignStart="@+id/textView27"
                    android:layout_alignLeft="@+id/textView27"
                    android:layout_alignEnd="@+id/textView27"
                    android:layout_alignRight="@+id/textView27"
                    android:layout_marginTop="12dp"
                    android:fontFamily="serif"
                    android:text="00:00"
                    android:textAlignment="center"
                    android:textColor="@android:color/black"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/textView72"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/textView71"
                    android:layout_alignBottom="@+id/textView71"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginLeft="20dp"
                    android:layout_marginStart="20dp"
                    android:fontFamily="serif"
                    android:text="1."
                    android:textAlignment="center"
                    android:textColor="@android:color/black"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/textView75"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textView71"
                    android:layout_alignStart="@+id/textView27"
                    android:layout_alignLeft="@+id/textView27"
                    android:layout_alignEnd="@+id/textView27"
                    android:layout_alignRight="@+id/textView27"
                    android:layout_marginTop="5dp"
                    android:fontFamily="serif"
                    android:text="00:00"
                    android:textAlignment="center"
                    android:textColor="@android:color/black"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/textView76"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/textView75"
                    android:layout_alignBottom="@+id/textView75"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginLeft="20dp"
                    android:layout_marginStart="20dp"
                    android:fontFamily="serif"
                    android:text="2."
                    android:textAlignment="center"
                    android:textColor="@android:color/black"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/textView77"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textView75"
                    android:layout_alignStart="@+id/textView27"
                    android:layout_alignLeft="@+id/textView27"
                    android:layout_alignEnd="@+id/textView27"
                    android:layout_alignRight="@+id/textView27"
                    android:layout_marginTop="5dp"
                    android:fontFamily="serif"
                    android:text="00:00"
                    android:textAlignment="center"
                    android:textColor="@android:color/black"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/textView78"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/textView77"
                    android:layout_alignBottom="@+id/textView77"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginStart="20dp"
                    android:layout_marginLeft="20dp"
                    android:fontFamily="serif"
                    android:text="3."
                    android:textAlignment="center"
                    android:textColor="@android:color/black"
                    android:textSize="20sp" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relativeLayout9"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/relativeLayout10"
                android:layout_marginEnd="5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="10dp"
                android:background="@android:color/white"
                android:paddingBottom="10dp">

                <TextView
                    android:id="@+id/textView25"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textView26"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="5dp"
                    android:fontFamily="serif"
                    android:paddingStart="5dp"
                    android:paddingLeft="5dp"
                    android:paddingEnd="5dp"
                    android:paddingRight="5dp"
                    android:text="0"
                    android:textColor="@android:color/black"
                    android:textSize="24sp" />

                <TextView
                    android:id="@+id/textView26"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="10dp"
                    android:ellipsize="end"
                    android:fontFamily="serif"
                    android:gravity="center_vertical"
                    android:maxLines="1"
                    android:paddingEnd="10dp"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:paddingStart="10dp"
                    android:text="Promoções ativas"
                    android:textAlignment="center"
                    android:textColor="@android:color/black"
                    android:textSize="20sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/relativeLayout9"
                android:layout_marginEnd="5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="10dp"
                android:background="@android:color/white">

                <ImageView
                    android:id="@+id/imageView13"
                    android:layout_width="0.5dp"
                    android:layout_height="match_parent"
                    android:layout_above="@+id/imageView58"
                    android:layout_below="@+id/imageView102"
                    android:layout_alignParentStart="true"
                    app:srcCompat="@color/amarelo" />

                <ImageView
                    android:id="@+id/imageView14"
                    android:layout_width="0.5dp"
                    android:layout_height="match_parent"
                    android:layout_below="@+id/imageView102"
                    android:layout_alignBottom="@+id/imageView58"
                    android:layout_alignParentEnd="true"
                    app:srcCompat="@color/amarelo" />

                <ImageView
                    android:id="@+id/imageView102"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    app:srcCompat="@color/amarelo" />

                <Button
                    android:id="@+id/button"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/imageView102"
                    android:background="@drawable/button_clicked"
                    android:fontFamily="serif"
                    android:text="O pão saiu agora!"
                    android:textAllCaps="false"
                    android:textColor="@android:color/black"
                    android:textSize="18sp" />

                <ImageView
                    android:id="@+id/imageView103"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/button"
                    app:srcCompat="@color/amarelo" />

                <Button
                    android:id="@+id/button15"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/imageView103"
                    android:background="@drawable/button_clicked"
                    android:fontFamily="serif"
                    android:text="Notificar com hora"
                    android:textAllCaps="false"
                    android:textColor="@android:color/black"
                    android:textSize="18sp" />

                <ImageView
                    android:id="@+id/imageView104"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/button15"
                    app:srcCompat="@color/amarelo" />

                <Button
                    android:id="@+id/button16"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/button15"
                    android:background="@drawable/button_clicked"
                    android:fontFamily="serif"
                    android:text="Promoções"
                    android:textAllCaps="false"
                    android:textColor="@android:color/black"
                    android:textSize="18sp" />

                <ImageView
                    android:id="@+id/imageView105"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/button16"
                    app:srcCompat="@color/amarelo" />

                <Button
                    android:id="@+id/button23"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/imageView105"
                    android:background="@drawable/button_clicked"
                    android:fontFamily="serif"
                    android:text="Notificar mensagem/imagem"
                    android:textAllCaps="false"
                    android:textColor="@android:color/black"
                    android:textSize="18sp" />

                <ImageView
                    android:id="@+id/imageView106"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/button23"
                    app:srcCompat="@color/amarelo" />

                <Button
                    android:id="@+id/button36"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imageView106"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/button_clicked"
                    android:fontFamily="serif"
                    android:text="Chat com Cliente"
                    android:textAllCaps="false"
                    android:textColor="@android:color/black"
                    android:textSize="18sp" />

                <ImageView
                    android:id="@+id/imageView107"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/button36"
                    app:srcCompat="@color/amarelo" />

                <Button
                    android:id="@+id/button29"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imageView107"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/button_clicked"
                    android:fontFamily="serif"
                    android:text="Galeria"
                    android:textAllCaps="false"
                    android:textColor="@android:color/black"
                    android:textSize="18sp" />

                <ImageView
                    android:id="@+id/imageView58"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/button29"
                    android:layout_alignParentStart="true"
                    app:srcCompat="@color/amarelo" />
            </RelativeLayout>

        </RelativeLayout>

    </ScrollView>

</RelativeLayout>
