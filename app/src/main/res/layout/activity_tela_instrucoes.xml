<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/oak"
    tools:context="com.realvibedev.paoquente.TelaInstrucoes">

    <ImageView
        android:id="@+id/imageView41"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:srcCompat="@color/common_google_signin_btn_text_light_default" />

    <ImageView
        android:id="@+id/imageView40"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/button17"
        app:srcCompat="@drawable/intrucoess" />

    <ImageView
        android:id="@+id/imageView39"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/button17"
        app:srcCompat="@drawable/instrucoes" />

    <ImageView
        android:id="@+id/imageView38"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/button17"
        app:srcCompat="@drawable/intrucoesss" />

    <Button
        android:id="@+id/button17"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/space5"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/space5"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="2.5dp"
        android:background="@color/Marrom"
        android:fontFamily="serif"
        android:text="Pular"
        android:textAllCaps="false"
        android:textColor="@color/amarelo"
        android:textSize="24sp" />

    <Button
        android:id="@+id/button18"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="2.5dp"
        android:layout_marginRight="5dp"
        android:layout_toEndOf="@+id/space5"
        android:layout_toRightOf="@+id/space5"
        android:background="@color/Marrom"
        android:fontFamily="serif"
        android:text="Seguinte"
        android:textAllCaps="false"
        android:textColor="@color/amarelo"
        android:textSize="24sp" />

    <Space
        android:id="@+id/space5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
