<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.realvibedev.paoquente.Padaria.TelaPerfilPadaria">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/amarelologo"
        android:id="@+id/relativeLayout3">

        <TextView
            android:id="@+id/textView41"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginEnd="15dp"
            android:layout_marginRight="15dp"
            android:background="@drawable/button_clicked"
            android:elevation="10dp"
            android:fontFamily="serif"
            android:gravity="center"
            android:text="Salvar"
            android:textColor="@android:color/white"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/imageView31"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="9dp"
            android:layout_marginStart="9dp"
            app:srcCompat="@mipmap/back" />

        <TextView
            android:id="@+id/textView28"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:elevation="10dp"
            android:fontFamily="serif"
            android:gravity="center"
            android:text="Perfil"
            android:textColor="@android:color/white"
            android:textSize="24sp" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/relativeLayout3">

        <ImageView
            android:id="@+id/imageView32"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="20dp"
            app:srcCompat="@drawable/logo" />

        <ImageView
            android:id="@+id/imageView33"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignEnd="@+id/imageView32"
            android:layout_alignRight="@+id/imageView32"
            android:layout_alignTop="@+id/imageView32"
            app:srcCompat="@drawable/editpng" />

        <android.support.design.widget.TextInputLayout
            android:id="@+id/etNomeLayoutEditarPerfil"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView32"
            android:layout_centerHorizontal="true"
            android:layout_marginEnd="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:textColorHint="@android:color/darker_gray"
            android:theme="@style/MyEditTextTheme">

            <android.support.design.widget.TextInputEditText
                android:id="@+id/etNomeEditarPerfil"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:hint="Nome"
                android:inputType="textPersonName"
                android:textColor="@android:color/black"
                android:textColorHint="@android:color/black"
                android:textSize="24sp" />
        </android.support.design.widget.TextInputLayout>

        <Button
            android:id="@+id/button28"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/etNomeLayoutEditarPerfil"
            android:layout_centerHorizontal="true"
            android:layout_marginEnd="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="30dp"
            android:background="@drawable/button_clicked"
            android:text="Editar informações"
            android:textColor="@color/amarelo" />

        <ImageView
            android:id="@+id/imageView55"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_alignBottom="@+id/button28"
            android:layout_alignEnd="@+id/button28"
            android:layout_alignLeft="@+id/button28"
            android:layout_alignRight="@+id/button28"
            android:layout_alignStart="@+id/button28"
            android:layout_alignTop="@+id/button28"
            android:layout_marginBottom="-1dp"
            android:layout_marginEnd="-1dp"
            android:layout_marginLeft="-1dp"
            android:layout_marginRight="-1dp"
            android:layout_marginStart="-1dp"
            android:layout_marginTop="-1dp"
            app:srcCompat="@color/amarelo" />

        <ImageView
            android:id="@+id/imageView56"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_alignBottom="@+id/button28"
            android:layout_alignEnd="@+id/button28"
            android:layout_alignLeft="@+id/button28"
            android:layout_alignRight="@+id/button28"
            android:layout_alignStart="@+id/button28"
            android:layout_alignTop="@+id/button28"
            app:srcCompat="@android:color/white" />

        <ProgressBar
            android:id="@+id/progressBar5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView33"
            android:layout_centerHorizontal="true"
            android:theme="@style/MyEditTextTheme"
            android:visibility="invisible" />

        <Button
            android:id="@+id/button11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/button28"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/button_clicked"
            android:text="Galeria"
            android:textColor="@color/amarelo" />

        <ImageView
            android:id="@+id/imageView118"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_alignStart="@+id/button28"
            android:layout_alignLeft="@+id/button11"
            android:layout_alignTop="@+id/button11"
            android:layout_alignEnd="@+id/button28"
            android:layout_alignRight="@+id/button11"
            android:layout_alignBottom="@+id/button11"
            android:layout_marginStart="-1dp"
            android:layout_marginLeft="-1dp"
            android:layout_marginTop="-1dp"
            android:layout_marginEnd="-1dp"
            android:layout_marginRight="-1dp"
            android:layout_marginBottom="-1dp"
            app:srcCompat="@color/amarelo" />

        <ImageView
            android:id="@+id/imageView119"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_alignStart="@+id/button28"
            android:layout_alignLeft="@+id/button11"
            android:layout_alignTop="@+id/button11"
            android:layout_alignEnd="@+id/button28"
            android:layout_alignRight="@+id/button11"
            android:layout_alignBottom="@+id/button11"
            app:srcCompat="@android:color/white" />

        <Button
            android:id="@+id/button13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/button11"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/button_clicked"
            android:text="Atualizar Senha"
            android:textColor="@color/amarelo" />

        <ImageView
            android:id="@+id/imageView34"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_alignBottom="@+id/button13"
            android:layout_alignEnd="@+id/button13"
            android:layout_alignLeft="@+id/button13"
            android:layout_alignRight="@+id/button13"
            android:layout_alignStart="@+id/button13"
            android:layout_alignTop="@+id/button13"
            android:layout_marginBottom="-1dp"
            android:layout_marginEnd="-1dp"
            android:layout_marginLeft="-1dp"
            android:layout_marginRight="-1dp"
            android:layout_marginStart="-1dp"
            android:layout_marginTop="-1dp"
            app:srcCompat="@color/amarelo" />

        <ImageView
            android:id="@+id/imageView52"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_alignBottom="@+id/button13"
            android:layout_alignEnd="@+id/button13"
            android:layout_alignLeft="@+id/button13"
            android:layout_alignRight="@+id/button13"
            android:layout_alignStart="@+id/button13"
            android:layout_alignTop="@+id/button13"
            app:srcCompat="@android:color/white" />

        <Button
            android:id="@+id/button14"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/button13"
            android:layout_centerHorizontal="true"
            android:layout_marginEnd="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="30dp"
            android:background="@drawable/button_clicked"
            android:text="Atualizar Email"
            android:textColor="@color/amarelo" />

        <ImageView
            android:id="@+id/imageView53"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_alignBottom="@+id/button14"
            android:layout_alignEnd="@+id/button14"
            android:layout_alignLeft="@+id/button14"
            android:layout_alignRight="@+id/button14"
            android:layout_alignStart="@+id/button14"
            android:layout_alignTop="@+id/button14"
            android:layout_marginBottom="-1dp"
            android:layout_marginEnd="-1dp"
            android:layout_marginLeft="-1dp"
            android:layout_marginRight="-1dp"
            android:layout_marginStart="-1dp"
            android:layout_marginTop="-1dp"
            app:srcCompat="@color/amarelo" />

        <ImageView
            android:id="@+id/imageView54"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_alignBottom="@+id/button14"
            android:layout_alignEnd="@+id/button14"
            android:layout_alignLeft="@+id/button14"
            android:layout_alignRight="@+id/button14"
            android:layout_alignStart="@+id/button14"
            android:layout_alignTop="@+id/button14"
            app:srcCompat="@android:color/white" />

    </RelativeLayout>
</RelativeLayout>
